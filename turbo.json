{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": [
        "src/**",
        "tsup.config.ts",
        "../../tsconfig.json",
        "../../tsconfig.build.json",
        "package.json"
      ],
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}

{
  "name": "@dibdab/starter-kit",
  "description": "starter kit for tiptap",
  "version": "3.16.0",
  "homepage": "https://dibdab.dev",
  "keywords": [
    "dibdab",
    "tiptap starter kit"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@dibdab/core": "workspace:^",
    "@dibdab/extension-blockquote": "workspace:^",
    "@dibdab/extension-bold": "workspace:^",
    "@dibdab/extension-bullet-list": "workspace:^",
    "@dibdab/extension-code": "workspace:^",
    "@dibdab/extension-code-block": "workspace:^",
    "@dibdab/extension-document": "workspace:^",
    "@dibdab/extension-hard-break": "workspace:^",
    "@dibdab/extension-heading": "workspace:^",
    "@dibdab/extension-horizontal-rule": "workspace:^",
    "@dibdab/extension-italic": "workspace:^",
    "@dibdab/extension-link": "workspace:^",
    "@dibdab/extension-list": "workspace:^",
    "@dibdab/extension-ordered-list": "workspace:^",
    "@dibdab/extension-paragraph": "workspace:^",
    "@dibdab/extension-strike": "workspace:^",
    "@dibdab/extension-text": "workspace:^",
    "@dibdab/extension-underline": "workspace:^",
    "@dibdab/extensions": "workspace:^",
    "@dibdab/pm": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/dibdab",
    "directory": "packages/starter-kit"
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}

{
  "name": "@tiptap/react",
  "description": "React components for tiptap",
  "version": "2.0.0-beta.213",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap react components"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@tiptap/core": "^2.0.0-beta.213",
    "@tiptap/pm": "^2.0.0-beta.213",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "@tiptap/core": "^2.0.0-beta.209",
    "@tiptap/pm": "^2.0.0-beta.209",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@tiptap/extension-bubble-menu": "^2.0.0-beta.213",
    "@tiptap/extension-floating-menu": "^2.0.0-beta.213"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/tiptap",
    "directory": "packages/react"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "splitting": true,
    "format": [
      "esm",
      "cjs"
    ]
  }
}

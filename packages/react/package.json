{
  "name": "@dibdab/react",
  "description": "React components for tiptap",
  "version": "3.16.0",
  "homepage": "https://dibdab.dev",
  "keywords": [
    "dibdab",
    "tiptap react components"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./menus": {
      "types": {
        "import": "./dist/menus/index.d.ts",
        "require": "./dist/menus/index.d.cts"
      },
      "import": "./dist/menus/index.js",
      "require": "./dist/menus/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@types/use-sync-external-store": "^0.0.6",
    "fast-equals": "^5.3.3",
    "use-sync-external-store": "^1.4.0"
  },
  "devDependencies": {
    "@dibdab/core": "workspace:^",
    "@dibdab/pm": "workspace:^",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "optionalDependencies": {
    "@dibdab/extension-bubble-menu": "workspace:^",
    "@dibdab/extension-floating-menu": "workspace:^"
  },
  "peerDependencies": {
    "@dibdab/core": "workspace:^",
    "@dibdab/pm": "workspace:^",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "@types/react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "@types/react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/dibdab",
    "directory": "packages/react"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}

{
  "name": "@tiptap/extension-text-style",
  "description": "text style extension for tiptap",
  "version": "3.20.0",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap extension"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./background-color": {
      "types": {
        "import": "./dist/background-color/index.d.mts",
        "require": "./dist/background-color/index.d.cts"
      },
      "import": "./dist/background-color/index.mjs",
      "require": "./dist/background-color/index.cjs"
    },
    "./color": {
      "types": {
        "import": "./dist/color/index.d.mts",
        "require": "./dist/color/index.d.cts"
      },
      "import": "./dist/color/index.mjs",
      "require": "./dist/color/index.cjs"
    },
    "./font-family": {
      "types": {
        "import": "./dist/font-family/index.d.mts",
        "require": "./dist/font-family/index.d.cts"
      },
      "import": "./dist/font-family/index.mjs",
      "require": "./dist/font-family/index.cjs"
    },
    "./font-size": {
      "types": {
        "import": "./dist/font-size/index.d.mts",
        "require": "./dist/font-size/index.d.cts"
      },
      "import": "./dist/font-size/index.mjs",
      "require": "./dist/font-size/index.cjs"
    },
    "./line-height": {
      "types": {
        "import": "./dist/line-height/index.d.mts",
        "require": "./dist/line-height/index.d.cts"
      },
      "import": "./dist/line-height/index.mjs",
      "require": "./dist/line-height/index.cjs"
    },
    "./text-style": {
      "types": {
        "import": "./dist/text-style/index.d.mts",
        "require": "./dist/text-style/index.d.cts"
      },
      "import": "./dist/text-style/index.mjs",
      "require": "./dist/text-style/index.cjs"
    },
    "./text-style-kit": {
      "types": {
        "import": "./dist/text-style-kit/index.d.mts",
        "require": "./dist/text-style-kit/index.d.cts"
      },
      "import": "./dist/text-style-kit/index.mjs",
      "require": "./dist/text-style-kit/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@tiptap/core": "workspace:^"
  },
  "peerDependencies": {
    "@tiptap/core": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/tiptap",
    "directory": "packages/extension-text-style"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}

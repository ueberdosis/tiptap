{
  "name": "@tiptap/extension-code-block-lowlight",
  "description": "code block extension for tiptap",
  "version": "3.10.3",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap extension"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"

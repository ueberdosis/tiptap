{
  "name": "@tiptap/html",
  "description": "utility package to render tiptap JSON as HTML",
  "version": "3.0.0",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap utility"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {

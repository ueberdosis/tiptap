{
  "name": "@tiptap/html",
  "description": "utility package to render tiptap JSON as HTML",
  "version": "2.0.0-beta.46",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap utility"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "main": "dist/tiptap-html.cjs.js",
  "umd": "dist/tiptap-html.umd.js",
  "module": "dist/tiptap-html.esm.js",
  "types": "dist/packages/html/src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/tiptap-html.esm.js",
      "require": "./dist/tiptap-html.cjs.js",
      "default": "./dist/tiptap-html.esm.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@tiptap/core": "^2.0.0-beta.46",
    "hostic-dom": "^0.8.6",
    "prosemirror-model": "^1.14.1"
  }
}

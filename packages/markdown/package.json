{
  "name": "@dibdab/markdown",
  "description": "markdown parser and serializer for tiptap",
  "version": "3.16.0",
  "homepage": "https://dibdab.dev",
  "keywords": [
    "dibdab",
    "tiptap utility"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": {
        "browser": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "marked": "^15.0.12"
  },
  "devDependencies": {
    "@dibdab/core": "workspace:^",
    "@dibdab/pm": "workspace:^"
  },
  "peerDependencies": {
    "@dibdab/core": "workspace:^",
    "@dibdab/pm": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/dibdab",
    "directory": "packages/markdown"
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}

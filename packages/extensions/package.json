{
  "name": "@tiptap/extensions",
  "description": "various extensions for tiptap",
  "version": "3.20.0",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap extension"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./character-count": {
      "types": {
        "import": "./dist/character-count/index.d.mts",
        "require": "./dist/character-count/index.d.cts"
      },
      "import": "./dist/character-count/index.mjs",
      "require": "./dist/character-count/index.cjs"
    },
    "./drop-cursor": {
      "types": {
        "import": "./dist/drop-cursor/index.d.mts",
        "require": "./dist/drop-cursor/index.d.cts"
      },
      "import": "./dist/drop-cursor/index.mjs",
      "require": "./dist/drop-cursor/index.cjs"
    },
    "./focus": {
      "types": {
        "import": "./dist/focus/index.d.mts",
        "require": "./dist/focus/index.d.cts"
      },
      "import": "./dist/focus/index.mjs",
      "require": "./dist/focus/index.cjs"
    },
    "./gap-cursor": {
      "types": {
        "import": "./dist/gap-cursor/index.d.mts",
        "require": "./dist/gap-cursor/index.d.cts"
      },
      "import": "./dist/gap-cursor/index.mjs",
      "require": "./dist/gap-cursor/index.cjs"
    },
    "./undo-redo": {
      "types": {
        "import": "./dist/undo-redo/index.d.mts",
        "require": "./dist/undo-redo/index.d.cts"
      },
      "import": "./dist/undo-redo/index.mjs",
      "require": "./dist/undo-redo/index.cjs"
    },
    "./placeholder": {
      "types": {
        "import": "./dist/placeholder/index.d.mts",
        "require": "./dist/placeholder/index.d.cts"
      },
      "import": "./dist/placeholder/index.mjs",
      "require": "./dist/placeholder/index.cjs"
    },
    "./selection": {
      "types": {
        "import": "./dist/selection/index.d.mts",
        "require": "./dist/selection/index.d.cts"
      },
      "import": "./dist/selection/index.mjs",
      "require": "./dist/selection/index.cjs"
    },
    "./trailing-node": {
      "types": {
        "import": "./dist/trailing-node/index.d.mts",
        "require": "./dist/trailing-node/index.d.cts"
      },
      "import": "./dist/trailing-node/index.mjs",
      "require": "./dist/trailing-node/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@tiptap/core": "workspace:^",
    "@tiptap/pm": "workspace:^"
  },
  "peerDependencies": {
    "@tiptap/core": "workspace:^",
    "@tiptap/pm": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/tiptap",
    "directory": "packages/extension"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}
